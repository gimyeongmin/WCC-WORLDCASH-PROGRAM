<script type="c6bac045e47eca5b8333e3fc-text/javascript">
         var scrollingUp = 0;
         window.setInterval(scrollit, 8000);
         function scrollit() {
             if(scrollingUp == 0) {
                 $('#scroller').delay(3000).animate({ scrollTop: $("#scroller").scrollTop() + 26 }, 'slow');
             }
         }
        
         $('#scroller').bind('scroll', function () {
             $('#status').html(scrollingUp);
            
             if ($(this).scrollTop() + $(this).innerHeight() >= $(this)[0].scrollHeight) {
                 scrollingUp = 1;     
                 $('#scroller').delay(3000).animate({ scrollTop: 0 }, 1000, function() {
                     scrollingUp = 0;   
                 });
             }
         });
      </script>
