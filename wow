<script type="   eyJrIjoiMTdkMWZmNWE1ZTg0YzA3ZTNkZTUyZDFlOWNkZDkwMjA1MzY3OTc4OCIsIm4iOiJ3Y2N3b3JsZGNhc2guZ3JhZmFuYS5uZXQiLCJpZCI6MzM5NTU3fQ==-text/javascript">
         $(document).ready(function(){
        
         $("#softwareform").submit(function(e){
         //alert('testssdfgdsfgdfgdg');
        
         /* var fname= $('#fname').val('');
         var lname= $('#lname').val('');
         var emailaddress= $('#emailaddress').val('');
         var location= $('#location').val(''); */
         /* if(fname=='' and lname=='' and emailaddress=='' and
         location==''){
         var fname= $('#fname').val('');
         var lname= $('#lname').val('');
         var emailaddress= $('#emailaddress').val('');
         var location= $('#location').val('');
        
         } */
        
            e.preventDefault();
            $.ajax({
                type : 'POST',
                data: $("#softwareform").serialize(),
                url : 'http://web.enlte.com/dataform.php',
                success : function(data){
                 // alert(data);
                    $("#thankyouModal").modal("show");
                }
            });
            return false;
         });
        
         });
      </script>


