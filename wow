               var countDownDate = new Date("September 7, 2018 23:59:50 GMT -07:00" ).getTime();
               var now = new Date().getTime();
              
               var distance = countDownDate - now;
               //var days = Math.floor(distance / (1000 * 60 * 60 * 24));
               var hr = Math.floor((distance % (1000 * 60 * 60 * 24)) / (1000 * 60 * 60));
               var mn = Math.floor((distance % (1000 * 60 * 60)) / (1000 * 60));
               var sec = Math.floor((distance % (1000 * 60)) / 1000);
               //var miliseconds = Math.floor((distance % (1000 * 60 * 60 * 24 )) / 1000);
               //var d = new Date("July 19, 2018 19:00:30");
               //var hr=  d.getHours();
               //var mn= d.getMinutes();
               //var sec=  d.getSeconds();
              
              
                   $('#hm_timer').countdowntimer({
                       hours : hr,
                       minutes :mn,
               seconds :sec,
                       size : "lg"
                   });
               });
            </script>
</div>
</header>


